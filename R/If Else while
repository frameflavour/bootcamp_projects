# if else
score <- 85
ifelse(score >= 80 , "Passed", "Failed")

check_grade <- function(score) {
	if (score >= 80) {
		print("A")
	} else if (score >= 70) {
		print("B")
	} else if (score >= 60) {
		print("C")
	} else if (score >= 50) {
		print("D")
	} else {
		print("F")
	}
}


# while
x <- 0
while(x < 10) {
	print("Hello")
	x <- x + 1
}
